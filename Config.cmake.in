@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/PerfCountersTargets.cmake")

check_required_components(PerfCounter)

set(version 0.0.1)

set_property(TARGET PerfCounters PROPERTY VERSION ${version})
set_property(TARGET PerfCounters PROPERTY SOVERSION 3)
set_property(TARGET PerfCounters PROPERTY
  INTERFACE_PerfCounters_MAJOR_VERSION 3)
set_property(TARGET PerfCounters APPEND PROPERTY
  COMPATIBLE_INTERFACE_STRING PerfCounters_MAJOR_VERSION
)

# generate the version file for the config file
write_basic_package_version_file(
  "${CMAKE_CURRENT_BINARY_DIR}/PerfCountersConfigVersion.cmake"
  VERSION "${version}"
  COMPATIBILITY AnyNewerVersion
)
